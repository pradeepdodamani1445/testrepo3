{
  "name": "LyraMaterial",
  "version": "1.0.4",
  "swift_versions": "5.0",
  "license": "BSD-3-Clause",
  "summary": "This library is a fork of CosmicMind Material library that fix the compilation problem with Xcode 15",
  "homepage": "https://github.com/lyra/Material",
  "authors": {
    "Lyra Dev Mobile": "dev-mobile@lyra-network.com"
  },
  "source": {
    "git": "https://github.com/lyra/Material.git",
    "tag": "1.0.4"
  },
  "default_subspecs": "Core",
  "platforms": {
    "ios": "11.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "platforms": {
        "ios": "11.0"
      },
      "ios": {
        "source_files": "Sources/**/*.swift"
      },
      "requires_arc": true,
      "resource_bundles": {
        "com.cosmicmind.material.icons": [
          "Sources/**/*.xcassets"
        ],
        "com.cosmicmind.material.fonts": [
          "Sources/**/*.ttf"
        ]
      },
      "dependencies": {
        "LyraMotion": [
          "~> 4.0.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
