  "description": "XYGamesKit 游戏 SDK.",
  "homepage": "https://github.com/MF-dosear/XYGamesKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Paul": "dosear@qq.com"
  },
  "source": {
