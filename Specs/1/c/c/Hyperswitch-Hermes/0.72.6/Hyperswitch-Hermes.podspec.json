{
  "name": "Hyperswitch-Hermes",
  "version": "0.72.6",
  "summary": "hermes-engine",
  "description": "hermes-engine for hyperswitchcore",
  "homepage": "https://hyperswitch.io/",
  "authors": "Hyperswitch",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "http": "https://repo1.maven.org/maven2/com/facebook/react/react-native-artifacts/0.72.6/react-native-artifacts-0.72.6-hermes-ios-debug.tar.gz"
  },
  "preserve_paths": "destroot/bin/*",
  "source_files": "destroot/include/**/*.h",
  "header_mappings_dir": "destroot/include",
  "ios": {
    "vendored_frameworks": "destroot/Library/Frameworks/universal/hermes.xcframework"
  },
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "compiler-default",
    "GCC_PREPROCESSOR_DEFINITIONS": "HERMES_ENABLE_DEBUGGER=1"
  }
}
