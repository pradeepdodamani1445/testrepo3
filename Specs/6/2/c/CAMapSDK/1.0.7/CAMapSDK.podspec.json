{
  "name": "CAMapSDK",
  "version": "1.0.7",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "地图融合SDK",
  "description": "封装常见地图SDK",
  "homepage": "https://github.com/datoujiejie/CAMAPSDK",
  "authors": "datoujiejie",
  "source": {
    "git": "git@github.com:datoujiejie/CAMAPSDK.git",
    "tag": "main"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
