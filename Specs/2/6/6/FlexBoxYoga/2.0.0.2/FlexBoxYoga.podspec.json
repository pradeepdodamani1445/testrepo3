{
  "name": "FlexBoxYoga",
  "version": "2.0.0.2",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://yogalayout.com/",
  "documentation_url": "https://yogalayout.com/docs",
  "summary": "An embeddable and performant flexbox layout engine with bindings for multiple languages",
  "authors": {
    "Meta Open Source": "opensource@meta.com"
  },
  "source": {
    "git": "https://github.com/BestYun/yoga.git",
    "tag": "v2.0.0.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "module_name": "yoga",
  "requires_arc": false,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "compiler_flags": [
    "-fno-omit-frame-pointer",
    "-fexceptions",
    "-Wall",
    "-Werror",
    "-Wextra",
    "-Wconversion",
    "-std=c++20",
    "-fPIC"
  ],
  "swift_versions": "5.1",
  "source_files": "yoga/**/*.{h,cpp}",
  "header_mappings_dir": "yoga",
