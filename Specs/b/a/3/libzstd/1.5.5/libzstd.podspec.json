{
  "name": "libzstd",
  "version": "1.5.5",
  "summary": "Cocoapods wrapper of facebook/zstd.",
  "description": "Zstandard, or zstd as short version, is a fast lossless compression algorithm, targeting real-time compression scenarios at zlib-level and better compression ratios. It\"s backed by a very fast entropy stage, provided by Huff0 and FSE library.",
  "homepage": "https://facebook.github.io/zstd/#other-languages",
  "license": "BSD",
  "authors": {
    "kongkaikai": "kongkaikai@bytedance.com"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.10"
  },
  "source": {
    "git": "https://github.com/facebook/zstd.git",
    "tag": "v1.5.5"
  },
  "source_files": [
    "lib/{compress,common,decompress,dictBuilder}/**/*.{h,c}",
    "lib/zstd.h",
    "lib/zstd_errors.h",
    "lib/zdict.h"
  ],
  "public_header_files": [
    "lib/zstd.h",
    "lib/zstd_errors.h",
    "lib/zdict.h"
  ],
  "swift_versions": "5.0",
  "xcconfig": {
    "GCC_C_LANGUAGE_STANDARD": "gnu11",
    "CLANG_CXX_LANGUAGE_STANDARD": "gnu++14"
  },
  "swift_version": "5.0"
}
